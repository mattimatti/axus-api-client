{
    "name": "mattimatti/axus-api-client",
    "version": "2.0.0",
    "type": "library",
    "description": "Axus Travel App API client",
    "homepage": "https://github.com/mattimatti/axus-api-client",
    "keywords": [
        "axus",
        "travel",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matteo Monti",
            "email": "developers@qofclubs.com",
            "homepage": "http://Axus.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Axus\\": "source/Axus/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axus\\tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.33",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "corneltek/phprelease": "^1.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/mattimatti/axus-api-client/issues",
        "wiki": "https://github.com/mattimatti/axus-api-client/wiki"
    }
}